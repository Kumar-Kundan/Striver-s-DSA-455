class Solution{
public:
	void pushZerosToEnd(int arr[], int n) {
	    // code here
	    int j=0,temp;
	    for(int i=0;i<n;++i){
	        
	        //if ith element is not equal to 0 then swap with jth element else increase i
	        if(arr[i]!=0){
	            temp=arr[i];
	            arr[i]=arr[j];
	            arr[j]=temp;
	            j++;
	        }
	    }
	}
};
